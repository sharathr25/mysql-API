  "description": "A website which allow you see some book details",
  "main": "server.js",
  "scripts": {
    "test": "mocha -w --recursive",
    "lint": "esw -w './**/*.js'",
    "coverage": "watch istanbul cover _mocha test/*",
    "lint-once": "eslint './**/*.js'",
    "test-once": "mocha --recursive ",
    "coverage-once": "istanbul cover _mocha test/*",
    "lint-test": "npm run lint && npm test",
    "create-database": "node create_database.js",
    "create-tables": "node create_tables.js",
    "insert-data-to-database": "node insert_to_table.js",
    "migrate": "npm run create-database && npm run create-tables && npm run insert-data-to-database",
    "start": "nodemon server.js",
    "production": "NODE_ENV='production' nodemon server.js"
  },
  "author": "Sharath",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "istanbul": "^1.1.0-alpha.1"
  },
  "dependencies": {
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "connect-redis": "^3.4.0",
    "ejs": "^2.6.1",
    "eslint": "^5.10.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.1",
    "istanbul": "^0.4.5",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "mongodb": "^3.1.10",
    "mongoose": "^5.4.0",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.4",
    "mysql2-promise": "^0.1.4",
    "npm-watch": "^0.5.0",
    "password-hash": "^1.2.2",
